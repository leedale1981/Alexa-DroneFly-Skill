{
  "name": "dronefly",
  "version": "1.0.0",
  "description": "Alexa UAV weather forcast skill",
  "main": "server.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon ./bin/www.js",
    "grunt": "grunt",
    "start": "node ./bin/www.js",
    "docker-build": "docker build -t leedale/dronefly .",
    "docker-tag": "docker tag leedale/dronefly leedale/dronefly:0.7.3",
    "docker-login": "docker login -u leedale",
    "docker-push": "docker push leedale/dronefly:0.7.3",
    "docker-run": "docker build -p 55555:8080 -d leedale/dronefly:0.7.3 --no-cache true"
  },
  "keywords": [
    "uav",
    "drone",
    "weather"
  ],
  "author": "Lee Dale",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.16.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "errorhandler": "^1.5.0",
    "es6-promise": "^4.1.0",
    "express": "^4.14.1",
    "method-override": "^2.3.7",
    "morgan": "^1.8.1",
    "pug": "^0.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.34",
    "@types/cookie-parser": "^1.3.30",
    "@types/core-js": "^0.9.37",
    "@types/errorhandler": "0.0.30",
    "@types/method-override": "0.0.29",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.5",
    "grunt": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-ts": "^5.5.1",
    "nodemon": "^1.11.0",
    "ts-node": "^2.1.0",
    "typescript": "^2.1.1"
  }
}
